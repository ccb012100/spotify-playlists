-- Find orphaned PlaylistTracks
-- sqlite3 ~/playlister.db ".read oprhaned_tracks.sqlite"
.headers on
.mode tabs

select
  pt.track_id,
  pt.playlist_id,
  pt.playlist_snapshot_id
from
  PlaylistTrack pt
  left join Playlist p on pt.playlist_snapshot_id = p.snapshot_id
where
  p.id is null

.output stdout
.headers off